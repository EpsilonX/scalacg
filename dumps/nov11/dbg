PackageDef(Select(Ident(tests), tests.tca), List(ClassDef(Modifiers(MODULE), newTypeName("ObjectInObject"), List(), Template(List(TypeTree()), emptyValDef, List(DefDef(Modifiers(), nme.CONSTRUCTOR, List(), List(List()), TypeTree(), Block(List(Apply(Select(Super(This(newTypeName("ObjectInObject")), tpnme.EMPTY), nme.CONSTRUCTOR), List())), Literal(Constant(())))), DefDef(Modifiers(), newTermName("main"), List(), List(List(ValDef(Modifiers(PARAM), newTermName("args"), TypeTree().setOriginal(AppliedTypeTree(Select(Ident(scala), scala.Array), List(TypeTree().setOriginal(Select(Select(This(newTypeName("scala")), scala.Predef), newTypeName("String")))))), EmptyTree))), TypeTree().setOriginal(Select(Ident(scala), scala.Unit)), Block(List(Apply(Select(Select(This(newTypeName("ObjectInObject")), tests.tca.ObjectInObject.A), newTermName("bar")), List())), Literal(Constant(())))), ClassDef(Modifiers(MODULE), newTypeName("A"), List(), Template(List(TypeTree()), emptyValDef, List(DefDef(Modifiers(), nme.CONSTRUCTOR, List(), List(List()), TypeTree(), Block(List(Apply(Select(Super(This(newTypeName("A")), tpnme.EMPTY), nme.CONSTRUCTOR), List())), Literal(Constant(())))), DefDef(Modifiers(), newTermName("bar"), List(), List(List()), TypeTree(), Block(List(ClassDef(Modifiers(MODULE), newTypeName("B"), List(), Template(List(TypeTree()), emptyValDef, List(DefDef(Modifiers(), nme.CONSTRUCTOR, List(), List(List()), TypeTree(), Block(List(Apply(Select(Super(This(newTypeName("B")), tpnme.EMPTY), nme.CONSTRUCTOR), List())), Literal(Constant(())))), DefDef(Modifiers(), newTermName("foo"), List(), List(List()), TypeTree(), Literal(Constant("foo")))))), ValDef(Modifiers(MUTABLE | MODULEVAR), newTermName("B$module"), TypeTree(), EmptyTree), DefDef(Modifiers(MODULE | TRIEDCOOKING), newTermName("B"), List(), List(List()), TypeTree(), Block(List(Assign(Ident(newTermName("B$module")), Apply(Select(New(TypeTree()), nme.CONSTRUCTOR), List()))), Ident(newTermName("B$module")))), ClassDef(Modifiers(MOD