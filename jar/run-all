#!/usr/bin/env bash

set -e
benchmarks=( argot ensime fimpp joos kiama phantm scalaxb scalisp see squeryl tictactoe )

date

echo "running analyses"
for benchmark in ${benchmarks[@]}
do
  ./run ${benchmark}
done

# run sanity checks
./sanity-all

# compile using scalac
./scalac-all

#echo "generating LaTeX files"
java -cp $SCALA_HOME/lib/scala-library.jar:$SCALA_HOME/lib/scala-reflect.jar:latex.jar ca.uwaterloo.scalacg.experiments.LatexGenerator
date